[Unit]
Description=GPS Data Monitor
After=network-online.target
Wants=network-online.target

[Service]
ExecStart=/home/pi/bin/python /home/pi/ippodromoScripts/mainUDP.py
WorkingDirectory=/home/pi/ippodromoScripts
